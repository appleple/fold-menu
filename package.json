    "html-minifier": "^3.5.21",
    "jest": "^23.6.0",
    "jest-puppeteer": "^3.7.0",
    "node-sass": "^4.13.1",
    "np": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
